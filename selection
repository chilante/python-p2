vetor=[50,30,40,20,10]
def BubleeSort(vetor):
  for i in range(len(vetor)):
    i+=1
    for j in range (len(vetor)-i):
      if vetor[j]>vetor[j+1]:
        aux=vetor[j]
        vetor[j] = vetor[j+1]
        vetor[j+1]= aux
  print(vetor)  
BubleeSort(vetor)
def SelectionSort(vetor):
  for i in range(len(vetor)-1):
    for j in range(len(vetor)):
      if vetor[j]<vetor[i]:
        aux=vetor[j]
        vetor[j]=vetor[i]
        vetor[i]=aux
SelectionSort(vetor)
